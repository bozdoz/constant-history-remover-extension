{
  "$schema": "https://json.schemastore.org/chrome-manifest.json",
  "manifest_version": 3,
  "name": "Constant History Remover",
  "description": "Generally keeps no history when creating/removing tabs",
  "version": "1.0.0",
  "homepage_url": "https://bozdoz.com",
  "permissions": ["browsingData", "tabs"],
  "background": {
    "service_worker": "background.js",
    "scripts": ["background.js"]
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "extensions@bozdoz.com"
    }
  }
}
